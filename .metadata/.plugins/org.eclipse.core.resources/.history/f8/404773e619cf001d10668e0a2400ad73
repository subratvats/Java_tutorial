package java_basics;

import java.util.Scanner;

public class LoopPractice {

    public static void main(String[] args) {
    	System.out.println("Enter the Number");
    	Scanner sc = new Scanner(System.in);
    	int n = sc.nextInt();
	    sc.close();
        
        for(int i=1;i<=10;i++)
        {
            System.out.println(n + "x" + i +" = "+ n*i);
        }
    }
    
}